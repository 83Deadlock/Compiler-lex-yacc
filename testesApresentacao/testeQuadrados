func main {
	decl {
		int a;
		int b;
		int c;
		int d;
	}
	inst {
		print("a = ");
		read(a);
		print("b = ");
		read(b);
		print("c = ");
		read(c);
		print("d = ");
		read(d);
		if((a == b) & (b == c) & (c == d)){
			print("É um quadrado!\n");
		} else {
			print("Não é um quadrado!\n");
		}
	}
}