func main {
	decl {
		int resultado = 1;
		int atual;
		int n;
	}
	inst {
		print("Quantos valores a considerar? ");
		read(n);
		repeat{
			print("inserir valor: ");
			read(atual);
			resultado = resultado * atual;
			n = n - 1;
		} until (n == 0)
		print("O resultado do produt√≥rio = ");
		print(resultado);
	}
}